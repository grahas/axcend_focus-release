    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Test Firmware Manager",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "cwd": "${workspaceFolder}",
            "args": [
                "-s", "axcend_focus_ros2_firmware_bridge/test/test_firmware_manager.py"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python: Test Legacy Compatability Interface",
            "type": "debugpy",
            "request": "launch",
            "module": "pytest",
            "cwd": "${workspaceFolder}",
            "args": [
                "-s", "axcend_focus_legacy_compatibility_layer/test/test_legacy_compatability_interface.py"
            ],
            "console": "integratedTerminal"
        },
        {
            "name": "Python Debugger: Current File",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
    ]
}